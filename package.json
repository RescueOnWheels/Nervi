{
  "name": "nervi",
  "version": "1.0.0",
  "description": "Nerve systems of the Rover.",
  "main": "index.js",
  "dependencies": {
    "debug": "3.1.0",
    "if-env": "1.0.4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0"
  },
  "optionalDependencies": {
    "pigpio": "1.1.4"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "if-env NODE_ENV=CI && npm run lint: || npm run lint:fix",
    "lint:": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "if-env NODE_ENV=CI && npm run test: || if-env npm_config_dev=true && npm run test:dev || if-env npm_config_debug=true && npm run test:debug || npm run test:",
    "test:": "nyc mocha",
    "test:debug": "cross-env DEBUG=RRS:* npm run test:",
    "test:dev": "if-env npm_config_debug=true && cross-env NODE_ENV=development npm run test:debug || cross-env NODE_ENV=development npm run test:"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RescueOnWheels/Nervi.git"
  },
  "author": "RescueOnWheels",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/RescueOnWheels/Nervi/issues"
  },
  "homepage": "https://github.com/RescueOnWheels/Nervi#readme",
  "directories": {
    "lib": "lib"
  }
}
